<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tomato.notice.mapper.NoticeRuleMapper">
    <select id="selectByRuleCode" resultType="NoticeRuleEntity">
        select
            *
        from
            t_notice_rule
        where
            rule_code = #{ruleCode}
    </select>
</mapper>